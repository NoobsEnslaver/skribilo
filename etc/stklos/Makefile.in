PRCS_FILES=Makefile.config.in Makefile.in Makefile.skb.in configure.in \
configure

all: configure


configure: configure.in
	autoconf

clean:
	/bin/rm -f config.* *~ 

pop:
	@echo $(PRCS_FILES:%=etc/stklos/%)

distclean: clean
	(cd ../../src/stklos/; $(MAKE) distclean)
	/bin/rm -f Makefile Makefile.skb ../Makefile.config
