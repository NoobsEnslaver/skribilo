#*    The meta Makefile for the sources                                */
#*=====================================================================*/
include ../etc/Makefile.config

#*---------------------------------------------------------------------*/
#*    pop                                                              */
#*---------------------------------------------------------------------*/
.PHONY: pop

pop:
	@ echo src/Makefile
	@ (cd bigloo && $(MAKE) pop)
	@ (cd stklos && $(MAKE) pop)

#*---------------------------------------------------------------------*/
#*    Install/Uinstall                                                 */
#*---------------------------------------------------------------------*/
.PHONY: install uninstall

install:
	(cd $(SYSTEM) && $(MAKE) install)

uninstall:
	(cd $(SYSTEM) && $(MAKE) uninstall)

#*---------------------------------------------------------------------*/
#*    clean                                                            */
#*---------------------------------------------------------------------*/
.PHONY: clean

clean:
	(cd $(SYSTEM) && $(MAKE) clean)

